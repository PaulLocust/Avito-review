# Logging
LOG_LEVEL=local

# Ports
HTTP_PORT=8080

# Database connection details
DB_HOST=postgres# менять localhost на postgres(название контейнера с бд) если использую docker
DB_USER=postgres
DB_PASSWORD=root
DB_NAME=avito_review
DB_PORT=5432
DB_SSL=disable

# Auto-generated DB URL
PG_URL=postgres://${DB_USER}:${DB_PASSWORD}@${DB_HOST}:${DB_PORT}/${DB_NAME}?sslmode=${DB_SSL}
PG_POOL_MAX=2

# Migrations
MIGRATIONS_PATH=migrations
